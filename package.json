{
  "name": "go-to-clipboard-file",
  "displayName": "Go to Clipboard File",
  "description": "Navigate to file:line references from clipboard",
  "version": "0.1.0",
  "publisher": "StefanoChiodino",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StefanoChiodino/go-to-clipboard-file.git"
  },
  "bugs": {
    "url": "https://github.com/StefanoChiodino/go-to-clipboard-file/issues"
  },
  "homepage": "https://github.com/StefanoChiodino/go-to-clipboard-file#readme",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:go-to-clipboard-file.goToFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "go-to-clipboard-file.goToFile",
        "title": "Go to File from Clipboard"
      }
    ],
    "keybindings": [
      {
        "command": "go-to-clipboard-file.goToFile",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g"
      }
    ],
    "configuration": {
      "title": "Go to Clipboard File",
      "properties": {
        "go-to-clipboard-file.stripPrefixes": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Path prefixes to strip from file paths (useful for container paths like '/app/', '/workspace/')"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js",
    "package": "rm -f *.vsix || true && vsce package"
  },

  "dependencies": {},
  "devDependencies": {
    "@vscode/vsce": "^2.18.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "16.x",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.74.0",
    "@vscode/test-electron": "^2.3.0",
    "esbuild": "^0.17.19",
    "glob": "^8.1.0",
    "mocha": "^11.5.0",
    "sinon": "^20.0.0",
    "typescript": "^4.9.4"
  }
}
